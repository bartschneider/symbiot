{
  "name": "synthora-react",
  "version": "1.0.0",
  "description": "Web Content Extraction Platform - Sitemap discovery and content scraping with Firecrawl",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "backend:setup": "cd backend && ./scripts/setup.sh",
    "backend:dev": "cd backend && make dev",
    "backend:build": "cd backend && make build",
    "backend:test": "cd backend && make test",
    "backend:db:reset": "cd backend && make db-reset",
    "dev:full": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "docker:up": "cd backend && docker-compose up -d",
    "docker:down": "cd backend && docker-compose down",
    "docker:logs": "cd backend && docker-compose logs -f"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.294.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-babel": "^1.3.2"
  },
  "keywords": [
    "react",
    "typescript",
    "web-scraping",
    "content-extraction",
    "sitemap-analysis",
    "firecrawl",
    "playwright",
    "go-backend",
    "full-stack"
  ],
  "author": "Symbiot",
  "license": "MIT"
}
